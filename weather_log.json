{
    "version": 1,              
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {
            "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
        }
    },
    "handlers": {
        "default": {
            "level":"DEBUG",    
            "class":"logging.StreamHandler",
            "formatter": "standard"
        }
    },
    "loggers": {
        "main": {                  
            "handlers": ["default"],
            "level": "DEBUG",
            "propagate": true
        }    
    }
}
